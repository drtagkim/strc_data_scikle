{
	"name":"Web Data Sickle",
	"description":"Data harvesting tool for Rvest, STRC 2019, Taekyung Kim",
	"version":"0.1.1",
	"manifest_version":2,
	"browser_action":{
		"default_popup":"index.html",
		"default_icons":{
			"16":"images/16.png",
			"32":"images/32.png",
			"64":"images/64.png"
		}
	},
	"background":{
		"scripts":["background.js"],
		"persistent":false
	},
	"icons":{
		"16":"images/16.png",
		"32":"images/32.png",
		"64":"images/64.png"
	},
	"permissions":[
		"storage",
		"activeTab",
		"http://*/",
		"https://*/"
	],
	"options_page":"options.html"
}